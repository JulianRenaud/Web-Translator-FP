@using RestSharp
@model Root
@{
    ViewData["Title"] = "Supported Languages";
    
}

<h1 class ="text-center">@ViewData["Title"]</h1>

<div class="text-center">
    <p>Here is a list with all of the supported languages!</p>
    <hr>
</div>


@foreach (var item in Model.data.languages)
{
    
    <h6 class="text-center">@item.name (@item.code)</h6>
}

